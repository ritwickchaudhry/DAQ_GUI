<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBlock x:Name="digital1" HorizontalAlignment="Left" Margin="21,11,0,0" TextWrapping="Wrap" Text="Digital Signal" VerticalAlignment="Top"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="83" Margin="10,11,0,0" VerticalAlignment="Top" Width="92"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="171,56,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="inputText" HorizontalAlignment="Left" Height="23" Margin="157,27,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="inputText_TextChanged"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="318,27,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="49" Width="99"/>

    </Grid>
</Window>
